<h1>Workout List</h1>
<p class="message" id="addMessage"></p>
<form>
	<fieldset>
		<legend>Edit Item</legend>
		<ul style="list-style-type: none;">
			<li>
				<label>Name: </label>
				<input type="text" name="name" id="name" value={{name}}>
			</li>
			<li>
				<label>Reps: </label>
				<input type="text" name="reps" id="reps" value={{reps}}>
			</li>
			<li>
				<label>Weight: </label>
				<input type="text" name="weight" id="weight" value={{weight}}>
				<label>Date: </label>
				<input type="radio" name="unit" id="lbs" value="lbs" checked>
				<label>Lbs: </label>
				<input type="radio" name="unit" id="kg" value="kg">
				<label>kg: </label>
			</li>
			<li>
				<label>Date: </label>
				<input type="date" name="date" id="date" {{date}}>
			</li>
			<input type="submit" id="insertSubmit">
		</ul>
	</fieldset>
</form>
 <script src="edit.js"></script>